<div class="row">
  <div class="col-md-8 col-md-offset-2">
	<%= link_to "Upload File", new_user_file_path(folder_id: @folder.id), class: 'btn btn-primary' %>
	<%= link_to "Create Folder", new_folder_path(folder_id: @folder.id), class: 'btn btn-primary' %>

	<div class="breadcrumbs">
	  <h4>
        <%= render 'shared/root_path' %>
        <%= render 'shared/path_display' %> <%= "#{@folder.name} /" %>	  		
	  </h4>
	</div>


	<%= render 'shared/display_contents' %>
  </div>
</div>